# Production Environment Variables
REACT_APP_VERSION=1.0.0
REACT_APP_API_URL=https://your-production-domain.com/api
REACT_APP_ENVIRONMENT=production

# External Service URLs
REACT_APP_KENWORTH_LOGO_URL=https://www.kenworth.com/media/w4jnzm4t/kenworth_logo-header-new-012023.png
REACT_APP_DECISIV_LOGO_URL=https://www.decisiv.com/wp-content/uploads/2020/01/Decisiv-Logo-096126.svg
REACT_APP_DECISIV_PORTAL_URL=https://paccar.decisiv.net/

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_PWA=true

# Analytics
REACT_APP_GA_TRACKING_ID=GA-XXXXXXXXX-X
REACT_APP_HOTJAR_ID=

# Browser Support
REACT_APP_TARGET_BROWSERS=">0.2%, not dead, not op_mini all"
